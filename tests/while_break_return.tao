function While() {
    let n = 10;
    while n > 0 {
        print(n);
        n = n - 1;
    }
}

function Break() {
    let n = 10;
    while n > 0 {
        print(n);
        break;
    }
}

function Return() {
    let n = 10;
    while n > 0 {
        print(n);
        return nil;
    }
}

function main() {
    While();
    Break();
    Return();
}
